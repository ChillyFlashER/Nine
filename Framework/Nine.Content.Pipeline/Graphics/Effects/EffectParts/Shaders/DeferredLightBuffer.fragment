fragment DeferredLightBuffer;

[parameters]
float SpecularPower;

[ps]
__hlsl__
void main(INPUT input, inout OUTPUT output)
{
	float4 color;
	import(Color, color = Color);
    color.a = SpecularPower / 255;
	export(float4, Color, color);
}
__hlsl__